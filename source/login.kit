        <div class="col-xs-12 col-sm-8 col-sm-offset-2 sign-in">
            <form class="sa-form">
              <fieldset>
               <legend>Sign In</legend>
               <div class="form-group">
                <label for="example">Email or Employee ID</label>
                <input type="email" class="form-control" id="example" placeholder="Email or Employee ID">
                <label for="exampleInputPassword1">Password</label>
                <input type="password" class="form-control" id="exampleInputPassword1" placeholder="Password">
               </div>
               <div class="form-group">
                <a class="btn btn-primary" href="pages/users/dashboard.html">Sign In</a>
                <a class="btn btn-xs pull-right btn-default" href="#">Forgot your password</a>
               </div>
              </fieldset>
            </form>
        </div> <!-- END Col sign-in -->
      </div> <!-- END Row -->


